  <script type="text/javascript">
  Stripe.setPublishableKey("{{stripe_publishable_key}}");
  if (ww.store.getItem('visitedUser')) {

  } else {
    var visitedUser = uuid.v4();
    ww.store.storeCookie('visitedUser', visitedUser);

  }
  if (sessionStorage.getItem('sessionId')) {

  } else {
    var sessionId = uuid.v4();
    sessionStorage.setItem('sessionId', sessionId);
  }
  Logger.debug('User Opened Public Page With Layout Onboarding');
  new onboarding('body');
  $(document).ready(function() {

    if (sessionStorage.getItem('token') && $.cookie('token')) {
      $('#login-action-public').addClass('hide');
      $('#dashboard-action').removeClass('hide');

    }

    $('.popoverbtn').popover({
      container: 'body'
    });
    $('.tooltipbtn').tooltip({
      container: 'body'
    });
    $(".chosen-select").chosen();
    var owl = $("#follow-carousel");
    var animateEnd = 'webkitAnimationEnd mozAnimationEnd MSAnimationEnd oanimationend animationend';

    $(".follow-suggestions li").addClass("animated fadeIn")
      .one(animateEnd, function() {
        $(this).removeClass("animated fadeIn");
      });

    $(".btn").click(function() {
      $(this).removeClass("btn-follow").addClass("btn-following-nohover");
      $(this).parents('div.owl-item').addClass("animated fadeOut")
        .one(animateEnd, function() {
          $(this).hide("fast");
        });
    });

    owl.owlCarousel({
      items: 4, //10 items above 1000px browser width
      itemsDesktop: [1000, 3], //5 items between 1000px and 901px
      itemsDesktopSmall: [900, 2], // betweem 900px and 601px
      itemsTablet: [600, 1], //2 items between 600 and 0
      itemsMobile: false, // itemsMobile disabled - inherit from itemsTablet option
      scrollPerPage: true
    });

    // Custom Navigation Events
    $(".next").click(function() {
      owl.trigger('owl.next');
    })
    $(".prev").click(function() {
      owl.trigger('owl.prev');
    })

    $(".cause-category li").addClass("animated fadeIn")
      .one(animateEnd, function() {
        $(this).removeClass("animated fadeIn");
      });


    $(".cause-category li").click(function() {
      $(this).toggleClass("selected")
    });

    $('.owl-item').css('height', equalHeight('item', 'owl-wrapper'));


    // WePay Validation in Organization Onboarding and WePay Account creation using Oauth2
    // If the Account is created already, we don not want to show the "WePay Connect" Button again and just show Dashboard button to access their account
    if ("{{stepfive}}") {

      if ("{{account_id}}") {

      } else {

        var me = this;

        WePay.set_endpoint("{{environment_type}}");

        // stage or production
        WePay.OAuth2.button_init(document.getElementById('start_oauth2'), {
          "client_id": "{{client_id}}",
          //"scope": "manage_accounts,collect_payments,view_user,preapprove_payments,manage_subscriptions,send_money"
          "scope": ["manage_accounts", "collect_payments", "view_user", "send_money", "preapprove_payments", "manage_subscriptions"],
          "user_name": "{{name}}",
          "user_email": "{{email}}",
          "redirect_uri": "{{domain}}",
          "top": 100, // control the positioning of the popup with the top and left params
          "left": 100,
          //  "state": "robot", // this is an optional parameter that lets you persist some state value through the flow
          "callback": function(data) {
            /** This callback gets fired after the user clicks "grant access" in the popup and the popup closes. The data object will include the code which you can pass to your server to make the /oauth2/token call **/
            console.log(data);

            if (data && data.error) {
              console.log('Error while authentication of wepay account creation');
              console.log(data.error);
              console.log(data);
              $('.wepay-error').html(data.error_description);
              $('.wepay-error').show();

            } else {
              $('.wepay-error').hide();

              if (data.code.length !== 0) {
                // send the data to the server

                $('#start_oauth2').hide();

                //{wepay_message_type: "oauth2_complete", code: "d28c71963ab15bc01fcece5dec50bb30c3e548328f83573b2b", state: "robot"}
                data.charityId = "{{charityId}}";
                data.title = "{{title}}";
                data.userId = "{{user_id}}";
                if (data.charityId) {
                  data.charityId = parseInt(data.charityId);
                }

                if (data.userId) {
                  data.userId = parseInt(data.userId);
                }

                $.ajax({
                  url: '/wepay/charity/account',
                  type: 'post',
                  dataType: 'json',
                  data: JSON.stringify(data),
                  success: function(data) {
                    $('.wepay-basic-view').hide();
                    $('.wepay-success-message').show();
                    $('.wepay-image').hide();
                  }
                });
              } else {
                // an error has occurred and will be in data.error
                console.log('Error while authentication of wepay account creation 2');
                console.log(data);
                $('.wepay-error').html(data.error_description);
                $('.wepay-error').show();
              }
            }
          }
        });
      }
    }
  });

  function statusChangeCallback(response) {
    console.log('statusChangeCallback');
    console.log(response);
    // The response object is returned with a status field that lets the
    // app know the current login status of the person.
    // Full docs on the response object can be found in the documentation
    // for FB.getLoginStatus().
    if (response.status === 'connected') {
      // Logged into your app and Facebook.
      testAPI();
    } else if (response.status === 'not_authorized') {
      // The person is logged into Facebook, but not your app.
      document.getElementById('status').innerHTML = 'Please log ' +
        'into this app.';
    } else {
      // The person is not logged into Facebook, so we're not sure if
      // they are logged into this app or not.
      document.getElementById('status').innerHTML = 'Please log ' +
        'into Facebook.';
    }
  }

  // This function is called when someone finishes with the Login
  // Button.  See the onlogin handler attached to it in the sample
  // code below.
  function checkLoginState() {
    FB.getLoginStatus(function(response) {
      statusChangeCallback(response);
    });
  }

  window.fbAsyncInit = function() {
    FB.init({
      appId: '1705678769667448',
      cookie: true, // enable cookies to allow the server to access
      // the session
      xfbml: true, // parse social plugins on this page
      version: 'v2.2' // use version 2.2
    });

    // Now that we've initialized the JavaScript SDK, we call
    // FB.getLoginStatus().  This function gets the state of the
    // person visiting this page and can return one of three states to
    // the callback you provide.  They can be:
    //
    // 1. Logged into your app ('connected')
    // 2. Logged into Facebook, but not your app ('not_authorized')
    // 3. Not logged into Facebook and can't tell if they are logged into
    //    your app or not.
    //
    // These three cases are handled in the callback function.

    FB.getLoginStatus(function(response) {
      statusChangeCallback(response);
    });

  };

  // Load the SDK asynchronously
  (function(d, s, id) {
    var js, fjs = d.getElementsByTagName(s)[0];
    if (d.getElementById(id)) return;
    js = d.createElement(s);
    js.id = id;
    js.src = "//connect.facebook.net/en_US/sdk.js";
    fjs.parentNode.insertBefore(js, fjs);
  }(document, 'script', 'facebook-jssdk'));

  // Here we run a very simple test of the Graph API after login is
  // successful.  See statusChangeCallback() for when this call is made.
  function testAPI() {
    console.log('Welcome!  Fetching your information.... ');

    FB.login(function(response) {
      console.log(response);
    }, {
      scope: 'user_friends,public_profile,email'
    });

    FB.api('/me', function(response) {
      console.log(response);
      document.getElementById('status').innerHTML =
        'Thanks for logging in, ' + response.name + '!';
    });
    FB.api('/me/taggable_friends', function(response1) {
      console.log('..................................');
      console.log(JSON.stringify(response1));
      console.log('..................................');
    });
  }
