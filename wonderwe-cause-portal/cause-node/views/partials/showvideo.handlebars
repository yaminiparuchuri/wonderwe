<div class="modal-dialog">
	<div class="modal-content">
		<div class="modal-header">
			<button class="close" id="close-video">
				<i class="fa fa-close"></i>
			</button>
			<h3><img src="" class="pull-left" style="margin-right:20px;">Please listen to {{videoData.name}}'s message </h3>
		</div>
		<div class="modal-body">
			<form class="form-horizontal">
				<div class="form-group">
					<div class="js-video-div" style="margin-left: 60px;">
						<ziggeo  ziggeo-video='{{videoData.video_token}}' responsive="true" ziggeo-width="440" ziggeo-height="330">
						</ziggeo>
					</div>
				</div>
			</form>
		</div>
	</div>
</div>
