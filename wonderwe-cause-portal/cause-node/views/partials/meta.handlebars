<head prefix="og: http://ogp.me/ns# fb: http://ogp.me/ns/fb# wonderwe: http://ogp.me/ns/fb/wonderwe#">
  <meta content="text/html; charset=utf-8" http-equiv="Content-Type">
  <title>{{metadata.seoTitle}} | WonderWe</title>
  <meta name="description" content="{{{stripTagsGreater metadata.seoDescription 155}}}" />
  <meta name="keywords" content="{{metadata.seoKeywords}}" />
  <!--For iPhone App -->
  <meta name="apple-itunes-app" content="app-id=1036116391, app-argument={{metadata.domain}}/{{metadata.seoSlugUrl}}"/>
  <meta property="al:ios:app_name" content="WonderWe"/>
  <meta property="al:ios:app_store_id" content="1036116391"/>
  <meta property="al:ios:url" content="ww://{{metadata.domain}}/{{metadata.seoSlugUrl}}"/>
  <!--For Android App -->
  <meta property="al:android:url" content="wonderwe://{{metadata.seoAndroidSlugUrl}}">
  <meta property="al:android:package" content="com.mobile.wonderwe">
  <meta property="al:android:app_name" content="WonderWe">
  <!-- for Google+ -->
  <meta itemprop="name" content="{{{metadata.seoSocialTitle}}}" />
  <meta itemprop="image" content="{{constructUrl metadata.seoImageUrl '275x275'}}" />
  <meta itemprop="description" content="{{{stripTagsGreater metadata.seoDescription  204}}}" />
  <!-- for Facebook -->
  <meta property="og:title" content="{{{metadata.seoSocialTitle}}}" />
  <meta property="og:type" content="{{metadata.seoOgType}}" />
  <meta property="og:description" content="{{{stripTagsGreater metadata.seoDescription 204}}}" />
  <meta property="og:image" content="{{constructUrl metadata.seoImageUrl '200x200'}}" />
  <meta property="og:image:width" content="200"/>
  <meta property="og:image:height" content="200"/>
  <meta property="og:url" content="{{metadata.domain}}/{{metadata.seoSlugUrl}}"/>
  <meta property="og:site_name" content="WonderWe"/>
  <meta property="og:locale" content="en_US" />
  <meta property="fb:app_id" content="855666327889009" />

  {{#if metadata.isCampaignCreator}}
  <meta property="wonderwe:creator" content="{{metadata.domain}}/{{metadata.seoCampaignCreatorSlug}}"/>
  {{/if}}

  <!-- for Twitter -->
  <meta name="twitter:card" content="summary_large_image" />
  <meta name="twitter:title" content="{{{metadata.seoSocialTitle}}}" />
  <meta name="twitter:image" content="{{constructUrl metadata.seoImageUrl '200x200'}}" />
  <meta name="twitter:description" content="{{{stripTagsGreater metadata.seoDescription 200}}}" />
  <!--Common To All Page -->
  <meta name="author" content="www.wonderwe.com" />
  <meta name="application-name" content="WonderWe" />
  {{#if metadata.indexStatus}}
  <meta name="robots" content="index,follow,all">
  {{else}}
  <meta name="robots" content="noindex,nofollow">
  {{/if}}
  <meta name="language" content="EN-US" />
  <meta name="geo.country" content="US" />
  <meta name="doc-type" content="Public" />
  <meta name="revisit-after" content="15 days">
  <meta name="copyright" content="2016 copyright">
  <meta name="distribution" content="Global">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0" />
  <link rel="canonical" href="{{metadata.domain}}/{{metadata.seoSlugUrl}}"/>


  <!-- build:css /dist/<<timeStamp>>/wonderwe_<<timeStamp>>.min.css -->
  <link href="/css/modal.css" rel="stylesheet" />
  <link href="/css/styles.css" rel="stylesheet" />
  <link href="/bower_components/chosen/chosen.min.css" rel="stylesheet" />
  <link href="/bower_components/iCheck/skins/square/grey.css" rel="stylesheet" />
  <link href="/bower_components/video.js/dist/video-js/video-js.min.css" rel="stylesheet" />
  <link href="/bower_components/Justified-Gallery/dist/css/justifiedGallery.css" rel="stylesheet" />
  <link href="/bower_components/colorbox/example1/colorbox.css" rel="stylesheet" />
  <!-- /build -->
  <link href="/bower_components/summernote/dist/summernote.css?v=1.2" rel="stylesheet">
  <link rel="icon" href="/images/favicons/favicon.ico" type="image/x-icon" />
  <link rel="shortcut icon" href="/images/favicons/favicon.ico" type="image/x-icon" />
  <link rel="apple-touch-icon" sizes="57x57" href="/images/favicons/apple-icon-57x57.png">
  <link rel="apple-touch-icon" sizes="60x60" href="/images/favicons/apple-icon-60x60.png">
  <link rel="apple-touch-icon" sizes="72x72" href="/images/favicons/apple-icon-72x72.png">
  <link rel="apple-touch-icon" sizes="76x76" href="/images/favicons/apple-icon-76x76.png">
  <link rel="apple-touch-icon" sizes="114x114" href="/images/favicons/apple-icon-114x114.png">
  <link rel="apple-touch-icon" sizes="120x120" href="/images/favicons/apple-icon-120x120.png">
  <link rel="apple-touch-icon" sizes="144x144" href="/images/favicons/apple-icon-144x144.png">
  <link rel="apple-touch-icon" sizes="152x152" href="/images/favicons/apple-icon-152x152.png">
  <link rel="apple-touch-icon" sizes="180x180" href="/images/favicons/apple-icon-180x180.png">
  <link rel="icon" type="image/png" sizes="192x192" href="/images/favicons/android-icon-192x192.png">
  <link rel="icon" type="image/png" sizes="32x32" href="/images/favicons/favicon-32x32.ico">
  <link rel="icon" type="image/png" sizes="128x128" href="/images/favicons/favicon-128x128.ico">
  <link rel="icon" type="image/png" sizes="16x16" href="/images/favicons/favicon-16x16.ico">
</head>