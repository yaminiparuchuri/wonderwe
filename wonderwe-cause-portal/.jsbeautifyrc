{
  "html": {
    "allowed_file_extensions": ["htm", "html", "xhtml", "shtml", "xml", "svg","mustache","hbs","handlebars"],
    "indent_char": "\t",
    "indent_size": 1
  },
  "css": {
    "allowed_file_extensions": ["css", "scss", "sass", "less"]
  },
  "js": {
    "allowed_file_extensions": ["js", "json", "jshintrc", "jsbeautifyrc"],
    "indent_char": " ",
    "indent_size": 2
  }
}